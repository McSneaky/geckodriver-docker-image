#!/bin/bash
# AUTOMATICALLY GENERATED
# DO NOT EDIT THIS FILE DIRECTLY, USE /post_push.tmpl.php

set -e

# Parse image name for repo name
tagStart=$(expr index "$IMAGE_NAME" :)
repoName=${IMAGE_NAME:0:tagStart-1}

# Tag and push image for each additional tag
for tag in {74.0-driver0.26.0-debian-buster,74.0-driver0.26.0-debian,74.0-driver0.26.0,74.0-debian-buster,74.0-debian,74.0,debian-buster,debian,latest}; do
  docker tag $IMAGE_NAME ${repoName}:${tag}
  docker push ${repoName}:${tag}
done
